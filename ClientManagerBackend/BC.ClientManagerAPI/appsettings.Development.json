{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      "Microsoft.EntityFrameworkCore": "Warning",
      "System": "Warning"
    },
    "File": {
      "FilePath": "C:\\Logs\\ClientManagerAPI\\app-.log",
      "RetainedFileCountLimit": 30,
      "FileSizeLimitBytes": 10485760,
      "OutputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz} [{Level:u3}] {Application} v{Version} [{SourceContext}] {Message:lj}{NewLine}{Exception}",
      "ConsoleOutputTemplate": "[{Level:u3}] {SourceContext}: {Message:lj}{NewLine}{Exception}",
      "EnableConsole": true,
      "DefaultLogLevel": "Information",
      "MicrosoftLogLevel": "Warning",
      "SystemLogLevel": "Warning"
    }
  },
  "Application": {
    "Name": "ClientManagerAPI",
    "Version": "1.0.0"
  },
  "AllowedHosts": "*",
  "DbConnection": "Server=localhost;Database=ClientManagerDB;Trusted_Connection=True;Encrypt=False;TrustServerCertificate=True;"
}
